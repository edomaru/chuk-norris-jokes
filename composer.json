{
    "name": "masaruedo/jokes",
    "description": "Create random chuk norris jokes",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Eding Muhamad",
            "email": "masaruedo@yahoo.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Masaruedo\\Jokes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Masaruedo\\Jokes\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "orchestra/testbench": "^6.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Masaruedo\\Jokes\\JokesServiceProvider"
            ],
            "aliases": {
                "ChuckNorris": "Masaruedo\\Jokes\\Facades\\ChuckNorris"
            }
        }
    }
}
